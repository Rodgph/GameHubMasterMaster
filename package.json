{
  "name": "gamehubmastermaster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "npm --prefix cloudflare run dev",
    "dev:desktop": "npm run tauri dev",
    "dev:desktop:api": "concurrently -k \"npm run tauri:dev\" \"npm run dev:api\"",
    "tauri:dev": "tauri dev",
    "tauri:dev:alt": "cross-env VITE_APP_INSTANCE=alt tauri dev --config src-tauri/tauri.alt.conf.json",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\""
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.16",
    "@supabase/supabase-js": "^2.97.0",
    "@tauri-apps/api": "^2.10.1",
    "dexie": "^4.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0",
    "zustand": "^5.0.11"
  }
}
